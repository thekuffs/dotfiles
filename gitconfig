[alias]
    b = branch
    ba = branch -a

    ci = commit

    l = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    lp = log --pretty=raw
    og = log origin/master..HEAD

    m = merge --no-ff

    s = status --short

    sub = submodule update --init --recursive
[color]
    ui = auto
[format]
    pretty = oneline
[hub]
	host = github.firehost.net
[github "github.firehost.net"]
	user = atygart
[push]
	default = tracking
